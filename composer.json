{
  "name": "merkeleon/laravel-log",
  "description": "Laravel Audit Log",
  "require": {
    "php": ">=7.1",
    "laravel/framework": ">=5.0",
    "ramsey/uuid": "^4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "4.5.*"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Merkeleon\\Log\\Providers\\MerkeleonLogProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Merkeleon\\Log\\": "src/"
    }
  },
  "minimum-stability": "dev"
}
